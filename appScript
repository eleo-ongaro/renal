// BOTON PARA MIGRAR DATOS DE LLAMADOS

function LimpiarCoordis(){
  
  var ss = SpreadsheetApp.getActiveSpreadsheet();
  var formS = ss.getSheetByName("Coordinadxr CETEC");
   
  var rangoLimpiar =  ["A2:BF"];
  for (let celda of rangoLimpiar ) {
  
    formS.getRange(celda).clearContent();

   }

}

function pasarCasos(){
  var h1 = SpreadsheetApp.getActive().getSheetByName("Planilla de casos");
  var h2 = SpreadsheetApp.getActive().getSheetByName("Coordinadxr CETEC");
  var uFila = h1.getLastRow();
  var tipoyfecha = h1.getRange("A2:B"+uFila).getValues();
  var accion = h1.getRange("IE2:IE"+uFila).getValues();
  var llamados = h1.getRange("C2:BD"+uFila).getValues();
  var dni = h1.getRange("BF2:BF"+uFila).getValues();
  
  var mat = [];

  for(var i=0 ; i<llamados.length;i++){
    mat.push((tipoyfecha[i]).concat(accion[i]).concat(llamados[i]).concat(dni[i]));
    
  }
Logger.log(mat);
h2.getRange(2,1,mat.length,mat[0].length).setValues(mat);
}


function traerLlamados() {
LimpiarCoordis();
SpreadsheetApp.getUi().alert('Casos borrados');
pasarCasos();
SpreadsheetApp.getUi().alert('Casos copiados desde Planilla de casos');
}

// ******************** OPE 1 **********************

// Limpiar celdas

function LimpiarCasillas() {
  var ss = SpreadsheetApp.getActiveSpreadsheet();
  var formS = ss.getSheetByName("Operadorx 1");

  var rangesToClear = ["N4:N54"];
   for (var i=0; i<rangesToClear.length; i++){
     formS.getRange(rangesToClear[i]).clearContent();
   }
}

function Limpiar() {
  var ss = SpreadsheetApp.getActiveSpreadsheet();
  var formS = ss.getSheetByName("Operadorx 1");

  var rangesToClear = ["C3","C6:C15","C17","C19","C23:D23","C25:D25","C26:C28","C30:D30","C32:D32","C33","C35:D35","C37:D38","C40:D40","C41:C43","F21","E3:G20","C54","C48:I50","C57:G78","C84:E88","H3","I3"];
   for (var i=0; i<rangesToClear.length; i++){
     formS.getRange(rangesToClear[i]).clearContent();
   }
}

// Buscar un caso y traer los datos

