    formulario.getRange("C88").getDisplayValue(),
    formulario.getRange("D88").getDisplayValue(),
    formulario.getRange("E88").getDisplayValue(),
    formulario.getRange("J48").getDisplayValue()
    ]]
                 
                 
                 

    datos.getRange(INT_R, 1, 1, 239).setValues(valores1);
      SpreadsheetApp.getUi().alert('Datos actualizados');
      Limpiar4();
    }
  }
}

// ******************** OPE 5 **********************

// Limpiar celdas

function LimpiarCasillas5() {
  var ss = SpreadsheetApp.getActiveSpreadsheet();
  var formS = ss.getSheetByName("Operadorx 5");

  var rangesToClear = ["N4:N54"];
   for (var i=0; i<rangesToClear.length; i++){
     formS.getRange(rangesToClear[i]).clearContent();
   }
}

function Limpiar5() {
  var ss = SpreadsheetApp.getActiveSpreadsheet();
  var formS = ss.getSheetByName("Operadorx 5");

  var rangesToClear = ["C3","C6:C15","C17","C19","C23:D23","C25:D25","C26:C28","C30:D30","C32:D32","C33","C35:D35","C37:D38","C40:D40","C41:C43","F21","E3:G20","C54","C48:I50","C57:G78","C84:E88","H3","I3"];
   for (var i=0; i<rangesToClear.length; i++){
     formS.getRange(rangesToClear[i]).clearContent();
   }
}

