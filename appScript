// ******************** OPE 3 **********************

// Limpiar celdas

function LimpiarCasillas() {
  var ss = SpreadsheetApp.getActiveSpreadsheet();
  var formS = ss.getSheetByName("Operadorx 1");

  var rangesToClear = ["N4:N54"];
   for (var i=0; i<rangesToClear.length; i++){
     formS.getRange(rangesToClear[i]).clearContent();
   }
}

function Limpiar3() {
  var ss = SpreadsheetApp.getActiveSpreadsheet();
  var formS = ss.getSheetByName("Operadorx 3");

  var rangesToClear = ["C3","C6:C15","C17","C19","C23:D23","C25:D25","C26:C28","C30:D30","C32:D32","C33","C35:D35","C37:D38","C40:D40","C41:C43","F21","E3:G20","C54","C48:I50","C57:G78","C84:E88","H3","I3"];
   for (var i=0; i<rangesToClear.length; i++){
     formS.getRange(rangesToClear[i]).clearContent();
   }
}

// Buscar un caso y traer los datos

var NUM_COLUMNA_BUSQUEDA = 57;

function Buscar3() {
  var hojaActiva = SpreadsheetApp.getActiveSpreadsheet();
  var formulario = hojaActiva.getSheetByName("Operadorx 3"); // Nombre de hoja del formulario
 
  var valor = formulario.getRange("C3").getValue();
  var valores = hojaActiva.getSheetByName("Planilla de casos").getDataRange().getDisplayValues(); // Nombre de hoja donde se almacenan datos
  // getRange("CELDA DEL FORMULARIO") setValue(Fila[COLUMNA DEL DATO QUE QUIERO PEGAR EN LA CELDA DEL FORMULARIO])
  for (var i = 0; i < valores.length; i++) {
     var fila = valores[i];
    if (fila[NUM_COLUMNA_BUSQUEDA] == valor) {

// Primera parte del formulario

     formulario.getRange("I3").setValue(fila[0]);
